<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Manual">

  <LinearLayout
      android:id="@+id/linearLayout2"
      android:layout_width="1338dp"
      android:layout_height="145dp"
      android:orientation="vertical"
      tools:layout_editor_absoluteX="8dp"
      tools:layout_editor_absoluteY="8dp">

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Manual Mode"
        android:textSize="70sp" />

  </LinearLayout>

  <LinearLayout
      android:id="@+id/linearLayout3"
      android:layout_width="1322dp"
      android:layout_height="114dp"
      android:descendantFocusability="beforeDescendants"
      android:focusableInTouchMode="true"
      android:orientation="horizontal"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.262"
      tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="178dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Set Speed:"
        android:textSize="25sp" />

    <EditText
        android:id="@+id/speedText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:ems="10"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/actuatorSpeed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Set Actuator Speed:"
        android:textSize="25sp" />

    <EditText
        android:id="@+id/actSpeedText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:ems="10"
        android:inputType="numberSigned" />

    <TextView
        android:id="@+id/spoolSizeText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Set Spool Size:"
        android:textSize="25sp" />

    <EditText
        android:id="@+id/spoolSizeNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:ems="10"
        android:inputType="numberSigned" />

  </LinearLayout>

  <Button
      android:id="@+id/jogForward"
      android:layout_width="156dp"
      android:layout_height="73dp"
      android:layout_marginEnd="216dp"
      android:layout_marginTop="196dp"
      android:text="Jog Forward"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

  <Button
      android:id="@+id/jogLeft"
      android:layout_width="156dp"
      android:layout_height="73dp"
      android:layout_marginBottom="88dp"
      android:layout_marginStart="836dp"
      android:text="Jog Left"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent" />

  <Button
      android:id="@+id/jogRight"
      android:layout_width="156dp"
      android:layout_height="70dp"
      android:layout_marginBottom="2dp"
      android:layout_marginEnd="68dp"
      android:text="Jog Right"
      app:layout_constraintBottom_toTopOf="@+id/jogReverse"
      app:layout_constraintEnd_toEndOf="parent" />

  <Button
      android:id="@+id/jogReverse"
      android:layout_width="156dp"
      android:layout_height="73dp"
      android:layout_marginBottom="16dp"
      android:layout_marginEnd="216dp"
      android:text="Jog Reverse"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent" />

  <Button
      android:id="@+id/setSpeedBtn"
      android:layout_width="206dp"
      android:layout_height="86dp"
      android:layout_marginBottom="340dp"
      android:text="Set Motor Speed"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.176"
      app:layout_constraintStart_toStartOf="parent" />

  <Button
      android:id="@+id/actSpeedBtn"
      android:layout_width="206dp"
      android:layout_height="83dp"
      android:layout_marginEnd="488dp"
      android:text="Set Actuator Speed"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

  <Button
      android:id="@+id/spoolSizeBtn"
      android:layout_width="206dp"
      android:layout_height="86dp"
      android:layout_marginEnd="68dp"
      android:text="Set Spool Size"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

  <Button
      android:id="@+id/runBtn"
      android:layout_width="220dp"
      android:layout_height="84dp"
      android:layout_marginBottom="184dp"
      android:layout_marginStart="356dp"
      android:text="Run"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent" />

  <Button
      android:id="@+id/stopBtn"
      android:layout_width="220dp"
      android:layout_height="84dp"
      android:layout_marginBottom="23dp"
      android:layout_marginStart="64dp"
      android:text="Stop"
      app:layout_constraintBottom_toTopOf="@+id/jogLeft"
      app:layout_constraintStart_toEndOf="@+id/runBtn" />

  <TextView
      android:id="@+id/textView11"
      android:layout_width="87dp"
      android:layout_height="wrap_content"
      android:layout_marginBottom="6dp"
      android:layout_marginStart="37dp"
      android:layout_weight="1"
      android:text="Length: "
      android:textSize="25sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent" />

  <TextView
      android:id="@+id/feed"
      android:layout_width="43dp"
      android:layout_height="wrap_content"
      android:layout_marginBottom="6dp"
      android:layout_marginStart="25dp"
      android:layout_weight="1"
      android:text="0"
      android:textSize="25sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toEndOf="@+id/textView11" />

  <TextView
      android:id="@+id/servoPos"
      android:layout_width="130dp"
      android:layout_height="42dp"
      android:layout_marginStart="51dp"
      android:text="Servo Pos:"
      android:textSize="25sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toEndOf="@+id/feed" />

  <TextView
      android:id="@+id/servoPosTxt"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="4dp"
      android:layout_marginStart="14dp"
      android:text="0"
      android:textSize="25sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toEndOf="@+id/servoPos" />

</android.support.constraint.ConstraintLayout>