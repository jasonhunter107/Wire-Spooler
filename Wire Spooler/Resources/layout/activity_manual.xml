<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Manual">

  <LinearLayout
      android:id="@+id/linearLayout2"
      android:layout_width="1338dp"
      android:layout_height="145dp"
      android:orientation="vertical"
      tools:layout_editor_absoluteX="8dp"
      tools:layout_editor_absoluteY="8dp">

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Manual Mode"
        android:textSize="70sp" />

  </LinearLayout>

  <LinearLayout
      android:id="@+id/linearLayout3"
      android:layout_width="1134dp"
      android:layout_height="114dp"
      android:orientation="horizontal"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.262"
      tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="84dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Set Speed:"
        android:textSize="40sp" />

    <EditText
        android:id="@+id/speedText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:ems="10"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="1dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Length: "
        android:textSize="40sp" />

    <TextView
        android:id="@+id/feed"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="0"
        android:textSize="40sp" />

  </LinearLayout>

  <Button
      android:id="@+id/jogForward"
      android:layout_width="156dp"
      android:layout_height="73dp"
      android:layout_marginEnd="216dp"
      android:layout_marginTop="196dp"
      android:text="Jog Forward"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

  <Button
      android:id="@+id/jogLeft"
      android:layout_width="156dp"
      android:layout_height="73dp"
      android:layout_marginBottom="88dp"
      android:layout_marginStart="836dp"
      android:text="Jog Left"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent" />

  <Button
      android:id="@+id/jogRight"
      android:layout_width="156dp"
      android:layout_height="70dp"
      android:layout_marginBottom="2dp"
      android:layout_marginEnd="68dp"
      android:text="Jog Right"
      app:layout_constraintBottom_toTopOf="@+id/jogReverse"
      app:layout_constraintEnd_toEndOf="parent" />

  <Button
      android:id="@+id/jogReverse"
      android:layout_width="156dp"
      android:layout_height="73dp"
      android:layout_marginBottom="16dp"
      android:layout_marginEnd="216dp"
      android:text="Jog Reverse"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent" />

  <Button
      android:id="@+id/runBtn"
      android:layout_width="206dp"
      android:layout_height="86dp"
      android:layout_marginBottom="284dp"
      android:text="Run"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent" />

</android.support.constraint.ConstraintLayout>